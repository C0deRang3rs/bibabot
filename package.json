{
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": "v15.0.1",
    "npm": "7.6.0"
  },
  "name": "bibabot",
  "repository": {
    "url": "https://github.com/C0deRang3rs/bibabot"
  },
  "scripts": {
    "start:prod": "node dist/index.js",
    "build:prod": "tsc",
    "start:dev": "node --require dotenv/config dist/index.js",
    "watch:dev": "tsc-watch --onSuccess 'npm run start:dev'",
    "watch:docker": "docker-compose up",
    "build:docker": "docker-compose build",
    "lint": "./node_modules/.bin/eslint --ext .ts ./",
    "test": "./node_modules/.bin/jest --silent",
    "predeploy": "npm run lint && npm run test",
    "release:dev": "heroku container:push bot -a bibabot-dev && heroku container:release bot -a bibabot-dev",
    "release:prod": "heroku container:push bot -a bibabot-prod && heroku container:release bot -a bibabot-prod"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.21.1",
    "az": "0.2.3",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "bull": "3.21.1",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "gm": "1.23.1",
    "hbs": "4.1.1",
    "lodash.zipobject": "4.1.3",
    "moment": "2.29.1",
    "puppeteer": "8.0.0",
    "redis": "3.0.2",
    "telegraf": "4.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.33",
    "@types/bull": "3.15.0",
    "@types/express": "4.17.11",
    "@types/gm": "1.18.9",
    "@types/jest": "26.0.21",
    "@types/lodash.zipobject": "4.1.6",
    "@types/node": "14.14.35",
    "@types/puppeteer": "5.4.3",
    "@types/redis": "2.8.28",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "ts-jest": "26.5.4",
    "tsc-watch": "4.2.9"
  },
  "eslintIgnore": [
    "__tests__/*"
  ]
}
